<map name="庆典海滩" category="1" id="-4" resId="-4">
	<fightMapColor>64,64,64,255</fightMapColor>
	<initialPoint>400,80</initialPoint>
	<music category="1">
		<bgm>MU_079</bgm>
	</music>
	<entities>
		<npc id="-401" resId="Pets/pet/160" name="因卡斯特" size="216,238" pos="725,165" namePos="0,-5" color="0,0,0,0">
			<description>因卡斯特</description>
			<dialog>
				<branch id="default" name="因卡斯特" icon="Pets/pet/160" size="108,119" pos="0,45">
					<content>因卡斯特居然隐藏在雕像之中</content>
					<functionHandler>
						<func type="click">
							<description><![CDATA[挑战因卡斯特]]></description>
							<action>openPanel</action>
							<param>TwelveRound</param>
							<param>pet=160</param>
							<param>pos=-10,70</param>
							<param>size=216,238</param>
							<param>item[2]=10239[1]</param>
							<param>item[5]=9008[3]</param>
							<param>item[8]=2[50]</param>
							<param>item[10]=10242[1]</param>
							<param>item[12]=500158[1]</param>
						</func>	
						<func type="click">
							<description><![CDATA[超进化]]></description>
							<action>battle</action>
							<param>evolve</param>
						</func>
					</functionHandler>
					<replyHandler>
						<reply type="click">
							<description><![CDATA[...学目灵雕像台词是吧？]]></description>
							<action>openDialog</action>
							<param>null</param>
						</reply>
					</replyHandler>
				</branch>
			</dialog>
			<battle>
				<branch id="default">
					<settings count="1">
						<mode>3</mode>
					</settings>
					<enemy>
						<pet id="606" level="70">
							<status>
								<hp>2000</hp>
							</status>
							<initBuff>61,12,13,14,17,90,99,960,962,-3000,-3059</initBuff>
							<headerSkill>12450</headerSkill>
							<loopSkill>12458,12459,12460</loopSkill>
						</pet>
					</enemy>
					<winHandler>
						<button type="auto">
							<action>getPet</action>
							<param>606,1</param>
							<condition>activity[11012].done=[bool]false[default]false</condition>
						</button>
						<button type="auto">
							<action>openHintbox</action>
							<param>type=Item</param>
							<param>title=提示</param>
							<param>content=布莱克已经放入精灵仓库</param>
							<param>item_icon=pet[606]</param>
							<param>option_num=1</param>
							<condition>activity[11012].done=[bool]false[default]false</condition>
						</button>
						<button type="auto">
							<action>setActivity</action>
							<param>11012</param>
							<param>done=true</param>
							<condition>activity[11012].done=[bool]false[default]false</condition>
						</button>
					</winHandler>
				</branch>
				<branch id="evolve">
					<settings count="3" star="2">
						<mode>3</mode>
					</settings>
					<enemy>
						<pet id="668" level="100">
							<status>
								<atk>600</atk>
								<mat>600</mat>
								<hp>6000</hp>
							</status>
							<initBuff>13,14,17,20,99,-3022,3486,3481</initBuff>
							<loopSkill>13330,13349,13355,13351</loopSkill>
						</pet>
					</enemy>
					<winHandler>
						<button type="auto">
							<action>openHintbox</action>
							<param>type=PetSelect</param>
						    <param>title=选择要进化的因卡斯特</param>
						    <param>option_num=2</param>
						    <param>callback=evolve</param>
						</button>
					</winHandler>
				</branch>
			</battle>
			<eventHandler>
				<button type="click">
					<action>openDialog</action>
					<param>default</param>
				</button>
			</eventHandler>
			<callbackHandler>
				<button type="choose">
					<action>openHintbox</action>
					<param>type=PetSelect</param>
				    <param>title=选择要进化的因卡斯特</param>
				    <param>option_num=2</param>
					<param>callback=evolve</param>
				    <condition>data.PetSelect.uidNOT160|data.PetSelect.level&lt;100</condition>
				</button>
				<button type="evolve">
				    <action>openHintbox</action>
				    <param>type=Item</param>
				    <param>title=提示</param>
				    <param>content=必须选择等级100的因卡斯特！</param>
				    <param>item_icon=pet[160]</param>
				    <param>option_num=1</param>
					<param>callback=choose</param>
				    <condition>data.PetSelect.uidNOT160|data.PetSelect.level&lt;100</condition>
				</button>
				<button type="evolve">
				    <action>openHintbox</action>
				    <param>type=Item</param>
				    <param>title=提示</param>
				    <param>content=因卡斯特已成功超进化</param>
				    <param>item_icon=pet[668]</param>
				    <param>option_num=1</param>
				    <condition>data.PetSelect.uid=160</condition>
				    <condition>data.PetSelect.levelGTE100</condition>
				</button>
				<button type="evolve">
				    <action>evolvePet</action>
				    <param>index=data.PetSelectIndex</param>
				    <param>668</param>
				    <condition>data.PetSelect.uid=160</condition>
				    <condition>data.PetSelect.levelGTE100</condition>
				</button>
			</callbackHandler>
		</npc>
		<!-- <teleport id="1" name="传送室" pos="720,60" targetMapId="-70"/> -->
	</entities>
</map>